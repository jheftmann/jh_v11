  },
  "homepage": "https://github.com/jheftmann/jh_v11#readme",
  "devDependencies": {
    "browser-sync": "^3.0.4",
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.10",
    "sass": "^1.89.2"
  }
}
