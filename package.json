{
  "name": "jh_v11",
  "version": "1.0.0",
  "description": "",
  "main": "js/script.js",
  "scripts": {
    "build:sass": "sass scss/style.scss css/style.css && node scripts/fix-quotes.js",
    "watch:sass": "concurrently \"sass --watch scss/style.scss css/style.css\" \"nodemon --watch '*.html' --watch '*.md' --ext html,md --exec 'node scripts/fix-quotes.js'\"",
    "start": "concurrently \"npm run watch:sass\" \"browser-sync start --server --files '*.html, css/*.css, js/*.js'\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jheftmann/jh_v11.git"
  },
  "keywords": [],
